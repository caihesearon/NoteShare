page{
  background-color: #ffffff;
}

.myPart{
  background-color:white;
  height: 350rpx;
  width: 100%;
  background-image: url('https://p.pstatp.com/origin/fff40000be7390f722b6');
  background-repeat:no-repeat; 
  background-size:100% 100%;
}

/*图像版块样式*/
.userinfo-image{
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 25px;
}

/*头像样式*/
.userinfo-image image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background-size: cover;
}

/*授权按钮样式*/
.loginBtn{
  margin-top:25rpx;
  display: flex;
  justify-content: center;
}

/*昵称版块样式*/
.userinfo-nickname{
  margin-top: 15rpx;
  display: flex;
  justify-content: center;
  color: #01AAED;
  background: rgba(0,0,0,0);
}

.NavigationBar{
  width: 94%;
  height: 370rpx;
  margin-left: 3%;
  box-shadow: 2px 2px 10px rgba(223, 219, 219, 0.911);
  background-color: #ffffff;
  border-radius: 40rpx;
  margin-top: 75rpx;
}

.content{
  margin-top: 30rpx;
}

.help{
  width: 94%;
  height: 260rpx;
  margin-left: 3%;
  box-shadow: 2px 2px 10px rgba(223, 219, 219, 0.911);
  background-color: #ffffff;
  border-radius: 40rpx;
  margin-top: 75rpx;
}
