page{
  background-color: #ffffff;
}

.myPart{
  background-color:white;
  height: 550rpx;
  width: 100%;
  /* background-image: url('https://p.pstatp.com/origin/fff40000be7390f722b6'); */
  background-image: url('https://ae01.alicdn.com/kf/Heac00a158dd849a485c48f3f8befe84bV.jpg'); 
  background-repeat:no-repeat; 
  background-size:100% 100%;
}

/*图像版块样式*/
.userinfo-image{
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 25px;
}

/*头像样式*/
.userinfo-image image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background-size: cover;
  position: relative;
  top: 115rpx;
  border: 2px solid white;
  box-shadow: 2px 2px 20px 8px rgba(255, 250, 250, 0.8)
}

/*授权按钮样式*/
.loginBtn{
  margin-top:140rpx;
  display: flex;
  justify-content: center;
}

/*昵称版块样式*/
.userinfo-nickname{
  margin-top: 135rpx;
  display: flex;
  justify-content: center;
  color: #01AAED;
  background: rgba(0,0,0,0);
  font-weight: 900;
  font-size: 1.2rem;
}


.showNotesNumberBox{
  width: 94%;
  height: 180rpx;
  box-shadow: 2px 2px 10px rgba(197, 187, 187, 0.8);
  display: flex;
  flex-direction: row;
  position: relative;
  left: 3%;
  bottom: 80rpx;
  background-color: white;
  border-radius: 10rpx;  
}
.totileNotes,.publicNotes,.collectNotes{
  width: 300rpx;
  height: 180rpx;
  /* border: 1px solid green; */
  /* line-height: 180rpx; */
  text-align: center;
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
}
.totileNotesNum,.totileNotesNum,.totileNotesNum{
  font-weight: bold;
  font-size: 1.5rem;
}

.Dec{
  margin-top: 6rpx;
}

.NavigationBar{
  width: 94%;
  height: 370rpx;
  margin-left: 3%;
  box-shadow: 2px 2px 10px rgba(197, 187, 187, 0.8);
  background-color: #ffffff;
  border-radius: 10rpx;
}

.content{
  margin-top: 30rpx;
}

.help{
  width: 94%;
  height: 260rpx;
  margin-left: 3%;
  box-shadow: 2px 2px 10px rgba(197, 187, 187, 0.8);
  background-color: #ffffff;
  border-radius: 10rpx;
  margin-top: 75rpx;
}
.cellBox{
  margin-top: 25rpx;
}
.Btn{
  width: 94%;
  height: 220rpx;
  display: flex;
  flex-direction: column;
  position: absolute;
  z-index: 1;  
}
.btnOne,.btnTwo{
  height: 130rpx;
  width: 100rpx;
  /* border: 1px solid red; */
}

/* 遮罩层 */
.overOneBox,.overTwoBox,.overThreeBox,.overFourBox{
  width: 100%;
  position: relative;
}
.overOneBox{
  top: 390rpx;
  left: 70rpx;
}
.overTwoBox{
  top: 580rpx;
  left: 35rpx;
}
.overThreeBox{
  top: 570rpx;
  left: 155rpx;
}
.overFourBox{
  top: 300rpx;
  left: 95rpx;
  display: flex;
  flex-direction: column;
}
