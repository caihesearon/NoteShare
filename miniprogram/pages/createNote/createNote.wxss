.editor-bottom{
  /* border: 1rpx solid blueviolet;   */
  display: flex;
  width: 100%;
  height: 300rpx;
}
.abs-container {
  position: absolute; 
  top: 100rpx; 
  left: 0rpx; 
  z-index: -1;
}
.container { 
  /* position: absolute;  */
  /* top: 100rpx;  */
  /* left: 0rpx;  */
  width: 100%;    
  /* border:1rpx solid blue; */
  padding: 10rpx;
  /* height: 1150rpx; */
  background-color: white;
}

.ql-container {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  font-size: 16px;
  line-height: 1.5;
  overflow: auto;
  padding: 10px 10px 20px 10px;
  border: 1px solid #ECECEC;
}

.ql-active {
  color: #22C704;
}

.iconfont {
  display: inline-block;
  width: 30px;
  height: 30px;
  cursor: pointer;
  font-size: 20px;
}

.fix-toolbar{
  position: fixed;
}
.toolbar {  
  /* position: fixed; */
  box-sizing: border-box;
  padding: 0 10px;
  height: 50px;
  width: 100%;  
  left: 0;
  right: 100%;  
  /* bottom: 0; */
  top: 0; /*决定功能框的位置*/
  display: flex;
  align-items: center;
  justify-content: space-between;
  border: 1px solid #ECECEC;
  border-left: none;
  border-right: none;
  background-color: white;
  z-index: 999;
}
.subbar{
  display: inline-block;
  position: fixed;
  top: 80rpx;
  height:80rpx;
  width: 240rpx;
  /* bottom:120rpx; */
  /* top:50rpx; */
  background-color: white;
  border-radius: 3px;
}
.ql-active {
  color: #06c;
}
.bar-active{
  background-color: #e9e9e9
}
.linebar{
  left:40rpx;
}
.placeholder{  
  font-weight: bolder;
  font-size: 1.6rem;
  height: 50rpx;  
}

.input-bottom{
  width: 250rpx;
  margin-right: 60rpx;
  font-size: 1.0rem;
  font-weight: normal;
  border-bottom: 1rpx solid gainsboro;
}

